<div class="row ">
    <div class="col-9">
        <h2 class="text-primary ">Company List</h2>
    </div>
    <div class="col-3">
        <a asp-action="Upsert" class="btn btn-outline-info offset-4">
            <i class="fas fa-plus"></i>  &nbsp; Add New Company
        </a>
    </div>
</div>
<div class="container rounded border p-2 m-2">
    <table id="tbldata" class="table table-bordered table-striped table-active">
        <thead>
            <tr>
                <th>Name</th>
                <th>StreetAddress</th>
                <th>City</th>
                <th>State</th>
                <th>PostalCode</th>
                <th>PhoneNumber</th>
                <th>IsAuthorizedCompany</th>
                <th>Actions</th>
            </tr>
        </thead>
    </table>
</div>
@section Scripts
{
    <script src="~/js/company.js"></script>
}
